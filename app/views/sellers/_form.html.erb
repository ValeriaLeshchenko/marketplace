<%= form_for current_seller, url: update_seller_path(current_seller), method: :put do |f| %>
  <p><%= f.label :email %> <%= f.text_field :email %></p>
  <p><%= f.label :password %> <%= f.password_field :password %></p>
  <p><%= f.label :phone %> <%= f.phone_field :phone %></p>
  <%= f.submit 'Update' %>
<% end %>